{
  "name": "DotNetCore_React",
  "version": "0.0.0",
  "scripts": {
    "gulp": "gulp",
    "compile": "webpack --config ./webpack.config.js",
    "compile:dll": "webpack --config ./webpack.dll.js",
    "compile:product": "cross-env NODE_ENV=product webpack --config ./webpack.config.js",
    "dotnet-watch": "dotnet run",
    "dev-watch": "",
    "dev-watch:server": "cross-env NODE_ENV=dev LOADER_ENV=happypack webpack-dev-server --progress --color --config ./webpack.config.js",
    "start": "concurrently \"npm run dotnet-watch\" \"npm run dev-watch\" ",
    "start:dev:dll": "npm run compile:dll & npm run dev-watch:server",
    "start:dev": "npm run dev-watch:server"
  },
